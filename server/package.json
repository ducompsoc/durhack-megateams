{
  "$schema": "https://json.schemastore.org/package",
  "name": "durhack-megateams-server",
  "description": "Server for DurHack Megateams project",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "Durham University Computing Society",
    "email": "computingsociety@durham.ac.uk",
    "url": "https://compsoc.tech/"
  },
  "contributors": [
    "Joe Clack <durhack@joeclack.me.uk>",
    "Jacob Wrenn <jacob@wrenn.me>",
    "Amber G <amber.g@durhack.com>",
    "Luca Huelle <lucahuelle@gmail.com>"
  ],
  "repository": {
    "url": "https://github.com/ducompsoc/durhack-megateams"
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "start": "npx cross-env NODE_ENV=production node ./dist/main.js",
    "dev": "tsx ./src/main",
    "dev:watch": "tsx watch ./src/main",
    "compile": "npx tsc -p tsconfig.json && npx tsc-alias -p tsconfig.json",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "build": "npm run format:write && npm run compile",
    "ci-test": "jest --passWithNoTests",
    "test": "jest",
    "all": "npm run format:write && npm run lint && npm run test && npm run compile"
  },
  "type": "module",
  "dependencies": {
    "@durhack/token-vault": "^0.1.2",
    "@otterhttp/app": "^3.4.4",
    "@otterhttp/cookie-signature": "^3.0.0",
    "@otterhttp/csrf-csrf": "^3.3.0",
    "@otterhttp/errors": "^0.2.1",
    "@otterhttp/parsec": "^0.1.2",
    "@otterhttp/session": "^0.3.0",
    "@prisma/client": "^5.19.0",
    "corstisol": "^1.0.0",
    "cross-env": "^7.0.3",
    "openid-client": "^5.6.5",
    "socket.io": "^4.7.5",
    "unique-names-generator": "^4.7.1",
    "uuid": "^9.0.0",
    "zod": "^3.22.2",
    "zod-config": "^0.0.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@types/node": "^20.5.9",
    "@types/uuid": "^9.0.3",
    "prisma": "^5.19.0",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4"
  },
  "packageManager": "pnpm@9.9.0+sha256.7a4261e50d9a44d9240baf6c9d6e10089dcf0a79d0007f2a26985a6927324177"
}
